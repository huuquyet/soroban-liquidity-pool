    "@types/node": "^20.11.26",
    "@types/react": "^18.2.65",
    "@types/react-currency-format": "^1.0.4",
    "@types/react-router-dom": "~5.3.3",
    "@vitejs/plugin-react": "^4.2.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "~2.2.3",
    "storybook": "^7.6.17",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.2",
    "vite": "^5.4.12",
