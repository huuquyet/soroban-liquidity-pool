    "remark-gfm": "^4.0.0",
    "sass": "^1.71.1",
    "share-token-contract": "file:.soroban/contracts/share-token",
    "token-a-contract": "file:.soroban/contracts/token-a",
    "token-b-contract": "file:.soroban/contracts/token-b",
    "web-vitals": "^3.5.2"
